    {
      "parameters": {
        "jsCode": "// Prepare AI Elements v2.2.0\nconst boardId = $('Create Miro Board').first().json.id;\nconst aiElements = $('Parse Input').first().json.aiElements || [];\nif (aiElements.length === 0) return [];\n\nconst validColors = ['gray', 'light_yellow', 'yellow', 'orange', 'light_green', 'green', 'dark_green', 'cyan', 'light_pink', 'pink', 'violet', 'red', 'light_blue', 'blue', 'dark_blue', 'black'];\n\nreturn aiElements.map((e, i) => {\n  const x = e.position?.x ?? 100 + (i % 3) * 420;\n  const y = e.position?.y ?? 100 + Math.floor(i / 3) * 220;\n  let color = (e.color || 'yellow').toLowerCase();\n  if (!validColors.includes(color)) color = 'yellow';\n  return { boardId, title: e.title || '', content: e.content || e.title || '', color, x, y, category: e.category };\n});"
      },
      "id": "prepare-ai-elements",
      "name": "Prepare AI Elements",
      "type": "n8n-nodes-base.code",
      "position": [896, 400],
      "typeVersion": 2
    },
    {
      "parameters": {
        "method": "POST",
        "url": "=https://api.miro.com/v2/boards/{{ $json.boardId }}/sticky_notes",
        "authentication": "predefinedCredentialType",
        "nodeCredentialType": "miroOAuth2Api",
        "sendBody": true,
        "specifyBody": "json",
        "jsonBody": "={\"data\": {\"content\": \"<strong>{{ $json.title }}</strong>\\n\\n{{ $json.content.substring(0, 250) }}\", \"shape\": \"square\"}, \"style\": {\"fillColor\": \"{{ $json.color }}\"}, \"position\": {\"x\": {{ $json.x }}, \"y\": {{ $json.y }}}, \"geometry\": {\"width\": 380}}"
      },
      "id": "add-ai-stickies",
