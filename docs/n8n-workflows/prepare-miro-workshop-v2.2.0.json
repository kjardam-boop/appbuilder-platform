        "jsCode": "// Prepare Response v2.2.0\nconst boardId = $('Create Miro Board').first().json.id;\nconst viewLink = $('Create Miro Board').first().json.viewLink;\nconst projectId = $('Parse Input').first().json.projectId;\nconst aiElements = $('Parse Input').first().json.aiElements || [];\nreturn { success: true, board_id: boardId, board_url: viewLink, project_id: projectId, elements_created: aiElements.length, status: 'board_ready', version: '2.2.0' };"
      },
