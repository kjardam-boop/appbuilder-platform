        "nodeCredentialType": "miroOAuth2Api",
        "sendBody": true,
        "specifyBody": "json",
        "jsonBody": "={\"data\": {\"content\": \"<strong>{{ $json.companyName }}</strong>\\n\\nProsjekt: {{ $json.projectName }}\\n\\nBransje: {{ $json.industry }}\", \"shape\": \"square\"}, \"style\": {\"fillColor\": \"light_blue\"}, \"position\": {\"x\": 100, \"y\": 100}, \"geometry\": {\"width\": 400}, \"parent\": {\"id\": \"{{ $json.frameId }}\"}}"
      },
