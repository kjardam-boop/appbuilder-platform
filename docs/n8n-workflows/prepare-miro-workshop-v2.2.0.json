        "jsCode": "// Parse Input v2.2.0\nconst input = $input.first().json;\nconst projectId = input.body?.input?.project_id;\nconst projectName = input.body?.input?.project_name || 'Prosjekt';\nconst companyId = input.body?.input?.company_id;\nconst companyName = input.body?.input?.company_name || 'Selskap';\nconst aiElements = input.body?.input?.ai_elements || [];\nconst aiSummary = input.body?.input?.ai_summary || '';\nconst context = input.body?.context || {};\nif (!projectId) throw new Error('project_id is required');\nreturn { projectId, projectName, companyId, companyName, aiElements, aiSummary, tenantId: context.tenant_id, userId: context.user_id };"
