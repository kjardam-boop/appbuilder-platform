      "id": "merge-inputs",
      "name": "Combine All Data",
      "type": "n8n-nodes-base.merge",
      "typeVersion": 3.1,
      "position": [750, 300]
    },
    {
      "parameters": {
        "mode": "runOnceForAllItems",
        "jsCode": "// Add timestamp and prepare for Dashboard\nconst items = $input.all();\nconst timestamp = new Date().toISOString();\n\nreturn items.map(item => ({\n  json: {\n    ...item.json,\n    Updated: timestamp\n  }\n}));"
      },
